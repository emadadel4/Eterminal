{
    "manifest_version": 2,
    "name": "Eterminal Tab",
    "version": "1.0",
    "description": "Make new tab like terminal and add your custom commands and more",
    "author": "Emad Adel",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "icons": {
    "32": "assets/icons/icon.jpg",
    "48": "assets/icons/icon.jpg",
    "96": "assets/icons/icon.jpg",
    "128": "assets/icons/icon.jpg"
  }
}
